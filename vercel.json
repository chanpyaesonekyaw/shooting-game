{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        },
        {
            "src": "*.js",
            "use": "@vercel/static"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/socket.io/(.*)",
            "dest": "server.js",
            "headers": {
                "Connection": "upgrade",
                "Upgrade": "$http_upgrade",
                "Sec-WebSocket-Key": "$http_sec_websocket_key",
                "Sec-WebSocket-Version": "$http_sec_websocket_version"
            }
        },
        {
            "src": "/game.js",
            "dest": "/game.js"
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}